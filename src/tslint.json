{
    "extends": "../tslint.json",
    "rudesDirectory": [
      "tslint-consistent-codestyle"
    ],
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "no-duplicate-imports": true,
        "ordered-imports": true,
        "no-require-imports": true,
        "no-unused": true,
        "no-collapsible-if": true,
        "no-unnecessary-else": true,
        "no-console": true
    }
}
